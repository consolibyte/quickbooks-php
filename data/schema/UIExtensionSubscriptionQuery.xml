                                <TimeCreated >DATETIMETYPE</TimeCreated> <!-- required -->
                                <TimeLastProcessed >DATETIMETYPE</TimeLastProcessed> <!-- optional -->
                                <COMCallbackInfo> <!-- required -->
                                        <AppName >STRTYPE</AppName> <!-- required -->
