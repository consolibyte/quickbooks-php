<AccountNumber >STRTYPE</AccountNumber> <!-- optional -->
<BankNumber >STRTYPE</BankNumber> <!-- optional -->
<Desc >STRTYPE</Desc> <!-- optional -->
<Balance >AMTTYPE</Balance> <!-- optional -->
